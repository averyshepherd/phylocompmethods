#NEXUS BEGIN DATA;	DIMENSIONS  NTAX=10 NCHAR=13;	FORMAT SYMBOLS= " 0 1 2 3 4" MISSING=? GAP=- ;	CHARSTATELABELS    1  amnion / absent present,  2  appendages / fins legs_only 'legs+wings',  3  body_covering / derm.scales smooth epid.scales feathers hair,  4  thermoreg. / poikilotherm homeotherm,  5  int.nostrils / absent present,  6  atrial_septum / absent present,  7  temp.fenestrae / none one two,  8  hemipenes / absent present,  9  suspensorium / not_strep. streptosylous,  10  gizzard / absent present,  11  antorb_fenes / absent present,  12  lat.sphen / ossified not,  13  teeth / not_ped. pedicillate		;MATRIX[                          10  ][                          .   ]rayfinned_fish    00000000?0010frogs             0110110000011turtles           1120110000010lungfish          00001100?0010salamanders       0110110000011crocodiles        1120112001100lizards           1120112110010birds             1231112001100mammals           1{12}41111000010snakes            1?20112110010;END;BEGIN ASSUMPTIONS;	USERTYPE Skintype (STEPMATRIX) = 5	     0 1 2 3 4	 [0] . 1 1 2 2	 [1] 1 . 1 1 1	 [2] 1 1 . 1 1	 [3] 2 1 1 . 1	 [4] 2 1 1 1 .	;END;BEGIN TREES; 	TRANSLATE		1	rayfinned_fish,		2	frogs,		3	turtles,		4	lungfish,		5	salamanders,		6	crocodiles,		7	lizards,		8	birds,		9	mammals,		10	snakes	;	TREE  * Bad_Guess =  [&R] (1,((5,(10,6)),((9,(7,3)),((2,8),4))));	TREE Another_Guess =  [&R] (1,(5,(((9,(7,3)),(10,6)),(8,(2,4)))));END;BEGIN PAUP;	constraints herps = ((frogs,turtles,salamanders,crocodiles,lizards,snakes));	constraints amniotes = ((turtles,crocodiles,lizards,birds,mammals,snakes));	constraints reptiles = ((turtles,crocodiles,lizards,snakes));	constraints archosaurs = ((crocodiles,birds));	constraints homeotherms = ((birds, mammals));	constraints hom+rept = ((birds, mammals),(turtles,crocodiles,lizards,snakes));END;